{
  "name": "UBOL_Dev_Extension",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "UBOL_Dev_Extension",
  "icons": {
    "128": "icons/toto128x128.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}